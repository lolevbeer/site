{
  "name": "lolev-beer",
  "version": "1.0.0",
  "description": "Lolev Beer brewery website with Payload CMS",
  "license": "MIT",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev -H 0.0.0.0",
    "build": "cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8000\" next build",
    "build:analyze": "ANALYZE=true cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8000\" next build",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lint:fix": "eslint --fix",
    "type-check": "tsc --noEmit",
    "preview": "pnpm build && pnpm start",
    "clean": "rm -rf .next",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "generate:beer-data": "tsx scripts/generate-beer-data.ts",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "test": "pnpm run test:int && pnpm run test:e2e",
    "test:e2e": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts",
    "test:a11y": "./scripts/test-accessibility.sh",
    "test:a11y:quick": "pnpm lint && pnpm type-check"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@monaco-editor/react": "4.7.0",
    "@payloadcms/db-mongodb": "3.72.0",
    "@payloadcms/next": "3.72.0",
    "@payloadcms/richtext-lexical": "3.72.0",
    "@payloadcms/storage-vercel-blob": "^3.72.0",
    "@payloadcms/ui": "3.72.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@sentry/nextjs": "^10.34.0",
    "@vercel/analytics": "^1.6.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "cross-env": "^10.1.0",
    "csv-parse": "^6.1.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "diff": "^8.0.3",
    "dotenv": "17.2.3",
    "embla-carousel-react": "^8.6.0",
    "graphql": "^16.12.0",
    "lucide-react": "^0.562.0",
    "mapbox-gl": "^3.18.0",
    "monaco-editor": "0.55.1",
    "next": "15.4.10",
    "next-themes": "^0.4.6",
    "nuqs": "^2.8.6",
    "papaparse": "^5.5.3",
    "payload": "3.72.0",
    "randomcolor": "^0.6.2",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "^7.71.1",
    "react-map-gl": "^8.1.0",
    "sharp": "0.34.5",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@axe-core/react": "^4.11.0",
    "@eslint/eslintrc": "^3.3.3",
    "@next/bundle-analyzer": "^16.1.3",
    "@playwright/test": "1.57.0",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.11",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/react": "16.3.1",
    "@types/node": "^25.0.9",
    "@types/papaparse": "^5.5.2",
    "@types/randomcolor": "^0.5.9",
    "@types/react": "19.2.8",
    "@types/react-dom": "19.2.3",
    "@vercel/blob": "^2.0.0",
    "@vitejs/plugin-react": "5.1.2",
    "eslint": "^9.39.2",
    "eslint-config-next": "15.4.10",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "jsdom": "27.4.0",
    "playwright": "1.57.0",
    "playwright-core": "1.57.0",
    "prettier": "^3.8.0",
    "tailwindcss": "^4.1.18",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "5.9.3",
    "vite-tsconfig-paths": "6.0.4",
    "vitest": "4.0.17",
    "webpack-bundle-analyzer": "^5.1.1"
  },
  "engines": {
    "node": ">=24",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild",
      "unrs-resolver"
    ]
  }
}
